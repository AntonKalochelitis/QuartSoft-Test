{
    "openapi": "3.0.0",
    "info": {
        "title": "Test Anton Kalochelitis API",
        "contact": {
            "email": "fire.anton@gmail.com"
        },
        "version": "0.1"
    },
    "paths": {
        "/api/auth/login": {
            "post": {
                "summary": "Sign in",
                "description": "Login by email, password",
                "operationId": "dd51a484b7fa0f97c7680fdbde14cf5f",
                "parameters": [
                    {
                        "name": "",
                        "in": "header",
                        "description": "Accept:application/json; Content-Type:multipart/form-data"
                    }
                ],
                "requestBody": {
                    "description": "Pass user credentials",
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "email",
                                    "password"
                                ],
                                "properties": {
                                    "email": {
                                        "type": "string",
                                        "format": "email",
                                        "example": "user1@mail.com"
                                    },
                                    "password": {
                                        "type": "string",
                                        "format": "password",
                                        "example": "PassWord12345"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Success response"
                    }
                }
            }
        },
        "/api/auth/register": {
            "post": {
                "summary": "Sign up",
                "description": "SingUP by nickname, email, password, password_confirmation",
                "operationId": "c7bb21a0d87049e161b8e4c9b0d909f2",
                "parameters": [
                    {
                        "name": "",
                        "in": "header",
                        "description": "Accept:application/json; Content-Type:multipart/form-data"
                    }
                ],
                "requestBody": {
                    "description": "Pass user credentials",
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "email",
                                    "password"
                                ],
                                "properties": {
                                    "nickname": {
                                        "type": "string",
                                        "format": "string",
                                        "example": "testuser1"
                                    },
                                    "email": {
                                        "type": "string",
                                        "format": "email",
                                        "example": "user1@mail.com"
                                    },
                                    "password": {
                                        "type": "string",
                                        "format": "password",
                                        "example": "PassWord12345"
                                    },
                                    "password_confirmation": {
                                        "type": "string",
                                        "format": "password",
                                        "example": "PassWord12345"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Success response"
                    }
                }
            }
        },
        "/api/auth/confirm/email": {
            "post": {
                "summary": "Email confirmation after registration",
                "description": "Confirmation after registration by token",
                "operationId": "12aba86038ef1c5e0fd187d2f4d33f2b",
                "parameters": [
                    {
                        "name": "",
                        "in": "header",
                        "description": "Accept:application/json; Content-Type:multipart/form-data"
                    }
                ],
                "requestBody": {
                    "description": "Pass user credentials",
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "token"
                                ],
                                "properties": {
                                    "token": {
                                        "type": "string",
                                        "format": "string",
                                        "example": "asdfasdfasd"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Success response"
                    }
                }
            }
        },
        "/api/auth/reset-password": {
            "post": {
                "summary": "Reset password",
                "description": "Reset forgot password",
                "operationId": "96e44e1ebcd811418fd95ee4c56162f0",
                "parameters": [
                    {
                        "name": "",
                        "in": "header",
                        "description": "Accept:application/json;"
                    }
                ],
                "requestBody": {
                    "description": "Pass user credentials",
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "email"
                                ],
                                "properties": {
                                    "email": {
                                        "type": "string",
                                        "format": "string",
                                        "example": "user1@mail.com"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Success response"
                    }
                }
            }
        },
        "/api/auth/logout": {
            "post": {
                "summary": "Sign out",
                "description": "Logout by token",
                "operationId": "df9bdc6c7a6d9f20b983526b2ec0f698",
                "parameters": [
                    {
                        "name": "",
                        "in": "header",
                        "description": "Accept:application/json;"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Success response"
                    }
                }
            }
        }
    }
}